{
  "C_Cpp.clang_format_path": "${workspaceRoot}/third_party/v8/buildtools/win/clang-format.exe",
  "C_Cpp.clang_format_style": "Google",
  "debug.allowBreakpointsEverywhere": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.formatOnSave": true,
  "editor.rulers": [80],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.endOfLine": "lf",
  "prettier.proseWrap": "always",
  "prettier.requireConfig": true,
  "python.formatting.provider": "yapf",
  "python.formatting.yapfPath": "${workspaceRoot}/third_party/python_packages/yapf",
  "python.linting.pylintPath": "${workspaceRoot}/third_party/python_packages/bin/pylint.bat",
  "python.linting.pylintArgs": [
    "--rcfile=${workspaceRoot}/depot_tools/pylintrc"
  ],
  "rust.clippy_preference": "on",
  "typescript.tsc.autoDetect": "off",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "xtree": "cpp",
    "utility": "cpp",
    "map": "cpp",
    "mutex": "cpp",
    "unordered_map": "cpp",
    "deque": "cpp",
    "list": "cpp",
    "vector": "cpp",
    "xhash": "cpp",
    "xstring": "cpp",
    "type_traits": "cpp",
    "xutility": "cpp",
    "array": "cpp",
    "forward_list": "cpp",
    "queue": "cpp",
    "stack": "cpp",
    "xmemory0": "cpp",
    "memory": "cpp",
    "algorithm": "cpp",
    "atomic": "cpp",
    "bitset": "cpp",
    "cctype": "cpp",
    "chrono": "cpp",
    "cinttypes": "cpp",
    "cmath": "cpp",
